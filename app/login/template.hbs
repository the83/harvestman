<form {{action "authenticate" on="submit"}}>
  <div class="row">
    <div class="input-field col s12">
      {{input value=model.identification type="text" id="email" name="email"}}
      <label class="active" for="email">E-mail</label>
    </div>
  </div>

  <div class="row">
    <div class="input-field col s12">
      {{input value=model.password type="password" id="password" name="password"}}
      <label class="active" for="password">Password</label>
    </div>
  </div>

  <div class="row">
    <div class="col s12">
      <button class="btn" type="submit">Log in</button>
    </div>
  </div>
</form>
